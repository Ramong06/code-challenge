gCount":0,"usedDeprecatedRules":"32"},{"filePath":"60","messages":"61","errorCount":0,"warningCount":1,"fixableErrorCount":0,"fixableWarningCount":0,"source":null},"/Users/ramon/Documents/react-apps/code-challenge/client/src/index.js",[],["62","63"],"/Users/ramon/Documents/react-apps/code-challenge/client/src/reportWebVitals.js",[],"/Users/ramon/Documents/react-apps/code-challenge/client/src/App.js",[],"/Users/ramon/Documents/react-apps/code-challenge/client/src/pages/Home.js",["64","65"],"import React, { useState, useEffect } from \"react\";\nimport DeleteBtn from \"../components/DeleteBtn\";\nimport Jumbotron from \"../components/Jumbotron\";\nimport API from \"../utils/API\";\nimport { Link } from \"react-router-dom\";\nimport { Col, Row, Container } from \"../components/Grid\";\nimport { List, ListItem } from \"../components/List\";\nimport Map from \"../components/Map\";\n\nconst Home = () => {\n  // Setting our component's initial state\n    const [businesses, setBusinesses] = useState([]);\n    const [formObject, setFormObject] = useState({});\n\n  // Load all businesses and stores them with setBusinesses\n  useEffect(() => {\n    loadBusinesses()\n  }, [])\n\n  // Loads all businesses and sets them to businesses\n  const loadBusinesses = () => {\n    API.getBusinesses()\n      .then(res => \n        setBusinesses(res.data)\n      )\n      .catch(err => console.log(err));\n  };\n\n    // Deletes a book from the database with a given id, then reloads books from the db\n  function deleteBusiness(id) {\n    API.deleteBusiness(id)\n      .then(res => loadBusinesses())\n      .catch(err => console.log(err));\n  }\n\n  // Handles updating component state for the input FORM when the user types into the input field for every key stroke\n  const handleInputChange = (event) => {\n    const { name, value } = event.target;\n    setFormObject({...formObject, [name]: value})\n  };\n\n  // When the form is submitted, use the API.saveBook method to save the book data\n  // Then reload books from the database\n  const handleFormSubmit = (event) => {\n    event.preventDefault();\n    if (formObject.businessName) {\n      API.saveBusiness({\n        businessName: formObject.name,\n        // address: formObject.address,\n        // phoneNumber: formObject.phoneNumber,\n        // website: formObject.website,\n        // image: formObject.image\n      })\n        .then(res => loadBusinesses())\n        .catch(err => console.log(err));\n    }\n  };\n\n    return (\n      <Container fluid>\n        <Row>\n          <Col size=\"md-6\">\n            <Jumbotron>\n              <h1>Look Up A Business</h1>\n            </Jumbotron>\n            <Map />\n          </Col>\n          <Col size=\"md-6 sm-12\">\n            <Jumbotron>\n              <h1>Business List</h1>\n            </Jumbotron>\n            {businesses.length ? (\n              <List>\n                {businesses.map(business => (\n                  <ListItem key={business._id}>\n                    <Link to={\"/businesses/\" + business._id}>\n                      <strong>\n                        {business.name}\n                      </strong>\n                      <br/>\n                      <strong>\n                        {business.address}\n                      </strong>\n                      <br/>\n                      <strong>\n                        {business.phoneNumber}\n                      </strong>\n                      <br/>\n                      <strong>\n                        {business.website}\n                      </strong>\n                    </Link>\n                    <DeleteBtn onClick={() => deleteBusiness(business._id)} />\n                  </ListItem>\n                ))}\n              </List>\n            ) : (\n              <h3>No Results to Display</h3>\n            )}\n          </Col>\n        </Row>\n      </Container>\n    );\n  }\n\n\nexport default Home;","/Users/ramon/Documents/react-apps/code-challenge/client/src/utils/API.js",["66"],"import axios from \"axios\";\n\nexport default {\n  // Gets all books\n  getBusinesses: function() {\n    return axios.get(\"/api/businesses\");\n  },\n  // Gets the book with the given id\n  getBusiness: function(id) {\n    return axios.get(\"/api/businesses\" + id);\n  },\n  // Deletes the book with the given id\n  deleteBusiness: function(id) {\n    return axios.delete(\"/api/businesses/\" + id);\n  },\n  // Saves a book to the database\n  saveBusiness: function(businessData) {\n    return axios.post(\"/api/businesses\", businessData);\n  }\n};\n\n\n","/Users/ramon/Documents/react-apps/code-challenge/client/src/pages/NoMatch.js",[],"/Users/ramon/Documents/react-apps/code-challenge/client/src/components/Jumbotron/index.js",[],"/Users/ramon/Documents/react-apps/code-challenge/client/src/components/List/index.js",[],"/Users/ramon/Documents/react-apps/code-challenge/client/src/components/Grid/index.js",[],"/Users/ramon/Documents/react-apps/code-challenge/client/src/components/Form/index.js",[],["67","68"],"/Users/ramon/Documents/react-apps/code-challenge/client/src/pages/Details.js",[],"/Users/ramon/Documents/react-apps/code-challenge/client/src/components/Nav/index.js",[],"/Users/ramon/Documents/react-apps/code-challenge/client/src/components/DeleteBtn/index.js",[],"/Users/ramon/Documents/react-apps/code-challenge/client/src/components/Map/index.js",["69"],{"ruleId":"70","replacedBy":"71"},{"ruleId":"72","replacedBy":"73"},{"ruleId":"74","severity":1,"message":"75","line":37,"column":9,"nodeType":"76","messageId":"77","endLine":37,"endColumn":26},{"ruleId":"74","severity":1,"message":"78","line":44,"column":9,"nodeType":"76","messageId":"77","endLine":44,"endColumn":25},{"ruleId":"79","severity":1,"message":"80","line":3,"column":1,"nodeType":"81","endLine":20,"endColumn":3},{"ruleId":"70","replacedBy":"82"},{"ruleId":"72","replacedBy":"83"},{"ruleId":"74","severity":1,"message":"84","line":4,"column":28,"nodeType":"76","messageId":"77","endLine":4,"endCol